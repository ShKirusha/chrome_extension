{
  "manifest_version": 2,
  "name": "Послушать Позже в vk.com",
  "description": "Позволяет вам добавлять музыку в новый раздел моих аудиозаписей - Послушать позже",
  "permissions": [ "storage","unlimited_storage", "tabs",
        "http://*/",
        "https://*/"],
  "version": "1.1",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
      "default_title": "Послушать Позже в vk.com",
      "default_popup": "popup.html"
   },
  "content_scripts": [
    {
      "matches": ["*://vk.com/*"],
      "js": ["jquery-2.2.2.min.js","vk_view.js","vk_inject.js"],
      "run_at": "document_end",
      "css": ["vk_styles.css"]
    }
  ],
  "background": {
    "scripts": [
      // "jquery-2.2.2.min.js",
      // "test.js",
      // "vk_inject.js"
    ]
  },
  "web_accessible_resources": [
    // "/jquery-2.2.2.min.js",
    "/vk_view.js",
    "/vk_inject.js"
  ]


}